@model IEnumerable<Assessment_1.Models.Customer>
@{
    ViewBag.Title = "CustomersResidingInGermany";
}

<h2>CustomersResidingInGermany</h2>
<table>
    <thead>
        <tr>
            <th>CustomerID</th>
            <th>CompanyName</th>
            <th>ContactName</th>
            <th>ContactTitle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
<tr>
    <td>@customer.CustomerID</td>
    <td>@customer.CompanyName</td>
    <td>@customer.ContactName</td>
    <td>@customer.ContactTitle</td>
</tr>}
    </tbody>
</table>
